{
  "name": "apple1js",
  "version": "1.1.1",
  "description": "NodeJS Apple 1 Emulator, include basic 6820 abstraction and other core components.",
  "main": "dist-web/bundle.js",
  "module": "src/index-web.js",
  "scripts": {
    "test": "run-s eslint type-check jest",
    "test-ci": "run-s eslint type-check jest-ci",
    "build": "webpack --config webpack.config.prod.js --mode production",
    "build-dev": "webpack --config webpack.config.dev.js  --mode development",
    "build-node": "webpack --config webpack-node.config.js --mode production",
    "start-dev": "webpack serve --config webpack.config.dev.js --mode development",
    "start": "node dist/bundle.js",
    "raw-start": "node index.js",
    "server-start": "http-server ./public",
    "eslint": "eslint src --ext .ts --ext .tsx --fix",
    "profile": "node --prof dist/bundle.js",
    "jest": "jest",
    "jest-ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit",
    "type-check": "tsc --build ./src/tsconfig.json",
    "deploy": "gh-pages -d public"
  },
  "repository": "git+https://github.com/stid/Apple1JS.git",
  "author": "Stefano Furiosi",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/cli": "7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-transform-async-to-generator": "7.16.0",
    "@babel/plugin-transform-runtime": "7.16.4",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@babel/register": "7.16.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.10",
    "@types/react": "^17.0.36",
    "@types/react-dom": "17.0.11",
    "@types/webpack-env": "1.16.3",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "8.2.3",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "4.1.0",
    "clean-webpack-plugin": "4.0.0",
    "dependency-cruiser": "^10.7.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "gh-pages": "3.2.3",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.0.0",
    "jest": "27.3.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.4.1",
    "terser-webpack-plugin": "^5.2.5",
    "typescript": "4.5.2",
    "webpack": "5.64.2",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "^4.5.0"
  },
  "dependencies": {
    "@babel/runtime": "7.16.3",
    "@babel/runtime-corejs3": "7.16.3",
    "@stitches/react": "^1.2.5",
    "core-js": "3.19.1",
    "eslint-plugin-import": "^2.25.3",
    "immer": "9.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "waait": "1.0.5"
  },
  "bugs": {
    "url": "https://github.com/stid/Apple1JS/issues"
  },
  "homepage": "https://github.com/stid/Apple1JS#readme",
